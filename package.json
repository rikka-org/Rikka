{
  "name": "rikka",
  "version": "0.1.0",
  "description": "Rikka is a fast, free as in freedom Discord client mod loader.",
  "main": "dist/index.js",
  "scripts": {
    "installrikka": "npm run build && node dist/Installer/installer.js",
    "uninstallrikka": "node dist/Installer/uninstall.js",
    "prepare": "ts-patch install -s",
    "build": "tsc -p tsconfig.json && npm run copy-files",
    "watch": "tsc -w -p tsconfig.json",
    "test": "jest",
    "copy-files": "copyfiles -u 1 src/**/*.pem src/**/*.css src/**/style.css src/**/.exists src/**/*.json src/**/*.scss dist/",
    "clean": "rimraf dist"
  },
  "keywords": [
    "discord",
    "discord-mod",
    "discord-injector"
  ],
  "author": "V3L0C1T13S",
  "license": "Apache-2.0",
  "devDependencies": {
    "@types/jest": "^27.4.1",
    "@types/node": "^17.0.21",
    "@types/prompt": "^1.1.2",
    "cpy-cli": "^4.1.0",
    "jest": "^27.5.1",
    "rimraf": "^3.0.2",
    "ts-jest": "^27.1.4",
    "ts-patch": "^2.0.1",
    "tsc-progress": "^1.0.4",
    "tslib": "^2.3.1",
    "typescript-transform-paths": "^3.3.1"
  },
  "dependencies": {
    "callsite": "^1.0.0",
    "codemirror": "^5.65.2",
    "copyfiles": "^2.4.1",
    "electron": "^17.1.1",
    "less": "^4.1.2",
    "node-watch": "^0.7.3",
    "object-assign": "^4.1.1",
    "redux": "^4.1.2",
    "sass": "^1.49.9",
    "stylus": "^0.57.0",
    "sucrase": "^3.20.3",
    "sudo-prompt": "^9.2.1",
    "typescript": "^4.6.2",
    "unzip-crx": "^0.2.0",
    "vm2": "^3.9.9"
  }
}
