{
  "name": "rikka",
  "version": "0.1.0",
  "description": "Rikka is a fast, free as in freedom Discord client mod loader.",
  "main": "dist/index.js",
  "scripts": {
    "rikka:install": "npm run build && node dist/Installer/installer.js",
    "rikka:uninstall": "node dist/Installer/uninstall.js",
    "prepare": "ts-patch install -s",
    "build": "tsc -p tsconfig.json && npm run copy-files",
    "watch": "tsc -w -p tsconfig.json",
    "copy-files": "copyfiles -u 1 src/**/*.pem src/**/*.css src/**/style.css src/**/.exists src/**/*.json src/**/*.scss src/**/*.coffee dist/",
    "clean": "rimraf dist",
    "lint": "eslint --ext .ts ./src",
    "doc": "tsdoc"
  },
  "keywords": [
    "discord",
    "discord-mod",
    "discord-injector"
  ],
  "author": "V3L0C1T13S",
  "license": "GPL-3.0",
  "devDependencies": {
    "@microsoft/tsdoc": "^0.14.1",
    "@microsoft/tsdoc-config": "^0.16.1",
    "@types/blessed": "^0.1.19",
    "@types/coffeescript": "^2.5.1",
    "@types/electron-devtools-installer": "^2.2.2",
    "@types/fs-extra": "^9.0.13",
    "@types/lodash": "^4.14.182",
    "@types/node": "^17.0.21",
    "@types/prompt": "^1.1.2",
    "@types/react": "^18.0.6",
    "@typescript-eslint/eslint-plugin": "^5.30.4",
    "@typescript-eslint/parser": "^5.22.0",
    "coffeescript": "^2.7.0",
    "cpy-cli": "^4.1.0",
    "eslint": "^8.19.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-plugin-import": "^2.26.0",
    "rimraf": "^3.0.2",
    "ts-patch": "^2.0.1",
    "tsc-progress": "^1.0.4",
    "tslib": "^2.3.1",
    "typedoc": "^0.23.7",
    "typescript-transform-paths": "^3.3.1"
  },
  "dependencies": {
    "blessed": "^0.1.81",
    "copyfiles": "^2.4.1",
    "electron": "^19.0.7",
    "electron-devtools-installer": "^3.2.0",
    "fs-extra": "^10.1.0",
    "git-repo-info": "^2.1.1",
    "lodash": "^4.17.21",
    "react": "^18.1.0",
    "react-is": "^18.2.0",
    "sass": "^1.53.0",
    "sucrase": "^3.23.0",
    "sudo-prompt": "^9.2.1",
    "typescript": "^4.7.4",
    "vm2": "^3.9.9"
  }
}
